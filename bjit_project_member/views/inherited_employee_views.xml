<record id="view_employee_type" model="ir.ui.view">
  <field name="name">hr.employee.form.inherit.employee.type</field>
  <field name="model">hr.employee.type</field>
  <field name="inherit_id" ref="hr.view_employee_form"/>
  <field name="arch" type="xml">
    <xpath expr="//notebook[1]" position="inside">
      <page name="Employee Type" string="Employee Type">
        <field name="name"/>
      </page>
    </xpath>
  </field>
</record>
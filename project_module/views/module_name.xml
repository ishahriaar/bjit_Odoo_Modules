<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>

<!-- for project form view -->
        <record id='module_name_entry_form' model='ir.ui.view' >
            <field name='name'>module.name.entry</field>
            <field name='model'>module.name</field>
            <field name='arch' type='xml'>
                <form>



                    <sheet>

                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>


                </form>
            </field>
        </record>

      <record id='module_name_tree_form' model='ir.ui.view' >
            <field name='name'>module.name.tree.view</field>
            <field name='model'>module.name</field>
            <field name='arch' type='xml'>
                <tree>




                            <field name="name"/>



                </tree>
            </field>
        </record>
  </data>
</odoo>
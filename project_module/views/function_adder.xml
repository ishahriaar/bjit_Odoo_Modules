<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>

<!-- for project form view -->
        <record id='fun_add_form' model='ir.ui.view' >
            <field name='name'>function.add.form</field>
            <field name='model'>function.add</field>
            <field name='arch' type='xml'>
                <form>



                    <sheet>

                        <group>
                            <field name="name"/>

                        </group>
                    </sheet>


                </form>
            </field>
        </record>


      <record id='fun_add_tree' model='ir.ui.view' >
            <field name='name'>function.add.tree</field>
            <field name='model'>function.add</field>
            <field name='arch' type='xml'>
                <tree>




                            <field name="name"/>



                </tree>
            </field>
        </record>
  </data>
</odoo>